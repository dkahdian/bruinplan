		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vite-plugin-devtools-json": "^0.2.0"
	},
	"dependencies": {
		"cytoscape": "^3.32.1",
